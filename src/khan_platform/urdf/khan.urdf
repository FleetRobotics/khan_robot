  </joint>

  <joint name="wheel4_joint" type="continuous">
    <parent link="base_link" />
    <child link="wheel4_link" />
    <origin xyz="-0.09275 -0.1125 0.030" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

</robot>
